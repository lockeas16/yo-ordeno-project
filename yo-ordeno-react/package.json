{
  "name": "yo-ordeno-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "password-validator": "^4.1.1",
    "qrcode.react": "^0.9.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "react-scroll-up-button": "^1.6.4",
    "react-step-wizard": "^5.3.0",
    "socket.io-client": "^2.2.0",
    "styled-components": "^4.3.1",
    "uikit": "^3.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp -r build/* ../yo-ordeno-api/public",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install-uikit-dep": "cd node_modules/uikit/ && yarn install",
    "build-custom-uikit": "rm -rf node_modules/uikit/custom && mkdir -p node_modules/uikit/custom && cp src/custom/my-theme.less node_modules/uikit/custom && cd node_modules/uikit/custom && yarn compile"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
